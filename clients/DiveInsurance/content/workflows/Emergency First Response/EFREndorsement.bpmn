def minute = calendar.get(Calendar.MINUTE);
minute = minute+1
hour = minute &gt; 59 ? hour+1 : hour
minute = minute &gt; 59 ? minute - 60 : minute
def cron = '0 '+(minute)+' '+hour+' '+day+' * ? 2020'</camunda:script>
