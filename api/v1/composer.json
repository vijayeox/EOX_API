{
  "name": "oxzion/core",
  "description": "Oxzion's Http APIs",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [
    "framework",
    "mvc",
    "zf"
  ],
  "homepage": "https://www.oxzion.com/",
  "minimum-stability": "dev",
  "prefer-stable": false,
  "config": {
    "secure-http" :false,
    "discard-changes": true
  },
  "repositories": [
  {
      "type": "pear",
      "url": "http://pear.horde.org"
  }],
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-component-installer": "^1.0 || ^0.7 || ^1.0.0-dev@dev",
    "zendframework/zend-mvc": "^3.0.1",
    "zfcampus/zf-development-mode": "^3.0",
    "zendframework/zend-db": "^2.9",
    "doctrine/migrations": "2.1",
    "zendframework/zend-json": "^3.1",
    "zendframework/zend-log": "^2.10",
    "ramsey/uuid":"^3.8.0",
    "zendframework/zend-authentication": "^2.6",
    "firebase/php-jwt": "^5.0",
    "zendframework/zend-permissions-rbac": "^2.6",
    "zendframework/zend-mail": "^2.10",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-cache": "^2.8",
    "zendframework/zend-serializer": "^2.9",
    "zendframework/zend-inputfilter": "^2.8",
    "pear-pear.horde.org/Horde_Injector": "2.*",
    "pear-horde/Horde_Injector": "2.*",
    "pear-pear.horde.org/Horde_Imap_Client":"*",
    "pear-pear.horde.org/Horde_Smtp":"*",
    "pear-pear.horde.org/Horde_Text_Filter": "^2.1",
    "elasticsearch/elasticsearch": "~7.0",
    "guzzlehttp/guzzle": "~6.0",
    "greenlion/php-sql-parser": "^4.1.2",
    "nelexa/zip": "dev-master",
    "stomp-php/stomp-php": "4.*",
    "johnkary/phpunit-speedtrap":"3.1.0",
    "ext-dom": "*",
    "ext-hash": "*",
    "ext-json": "*",
    "smarty/smarty": "~3.1",
    "tecnickcom/tcpdf": "^6.2.26",
    "friendsofphp/php-cs-fixer": "^2.16@dev",
    "knplabs/knp-snappy": "^1.0@dev",
    "h4cc/wkhtmltopdf-amd64": "0.12.x"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "zendframework/zend-test": "^3.2",
    "phpunit/dbunit": "^4",
    "mockery/mockery": "^1.2",
    "sempro/phpunit-pretty-print": "dev-master",
    "mybuilder/phpunit-accelerator": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Oxzion\\": "lib/Oxzion/src/",
      "Application\\": "module/Application/src/",
      "Form\\": "module/Form/src/",
      "Field\\": "module/Field/src/",
      "File\\": "module/File/src/",
      "Announcement\\": "module/Announcement/src/",
      "Alert\\": "module/Alert/src/",
      "User\\": "module/User/src/",
      "Group\\": "module/Group/src/",
      "Analytics\\": "module/Analytics/src/",
      "Organization\\": "module/Organization/src/",
      "Auth\\": "module/Auth/src/",
      "Attachment\\": "module/Attachment/src/",
      "App\\": "module/App/src/",
      "Project\\": "module/Project/src/",
      "Role\\": "module/Role/src/",
      "Search\\": "module/Search/src/",
      "Contact\\": "module/Contact/src/",
      "Email\\": "module/Email/src/",
      "Resource\\": "module/Resource/src/",
      "Privilege\\": "module/Privilege/src/",
      "Callback\\": "module/Callback/src/",
      "FileIndexer\\": "module/FileIndexer/src/",
      "Mlet\\": "module/Mlet/src/",
      "Messaging\\":"module/Messaging/src/",
      "ProspectResearch\\":"module/ProspectResearch/src/",
      "Workflow\\":"module/Workflow/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApplicationTest\\": "module/Application/test/",
      "FormTest\\": "module/Form/test/",
      "FieldTest\\": "module/Field/test/",
      "AnnouncementTest\\": "module/Announcement/test/",
      "ResourceTest\\": "module/Resource/test/",
      "FileTest\\": "module/File/test/",
      "UserTest\\": "module/User/test/",
      "GroupTest\\": "module/Group/test/",
      "AnalyticsTest\\": "module/Analytics/test/",
      "FileIndexerTest\\": "module/FileIndexer/test/",
      "RoleTest\\": "module/Role/test/",
      "OrganizationTest\\": "module/Organization/test/",
      "AuthTest\\": "module/Auth/test",
      "BookmarkTest\\": "module/Bookmark/test",
      "AppTest\\": "module/App/test",
      "Migration\\": "lib/Oxzion/test/Migration",
      "WorkflowTests\\": "module/Workflow/test",
      "WorkflowTest\\": "lib/Oxzion/test/WorkflowTest",
      "ProjectTest\\": "lib/Oxzion/test/ProjectTest",
      "EmailTest\\": "lib/Oxzion/test/EmailTest",
      "ContactTest\\": "module/Contact/test/",
      "MessageTest\\": "lib/Oxzion/test/MessageTest",
      "PrivilegeTest\\": "module/Privilege/test/",
      "ChatCallbackTest\\": "module/Callback/test/",
      "MessagingTest\\":"module/Messaging/test/"
    }
  },
  "extra": [],
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "phpFormatter.composer": true,
    "development-disable": "zf-development-mode disable",
    "development-enable": "zf-development-mode enable",
    "development-status": "zf-development-mode status",
    "post-create-project-cmd": [
      "@development-enable"
    ],
    "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpunit"
  }
}
